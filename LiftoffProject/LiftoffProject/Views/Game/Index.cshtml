@model List<LiftoffProject.Models.Game>

    <h1>@ViewBag.Title</h1>

<table class="table">
    <tr>
        <th>Name</th>
    </tr>

    @foreach (var game in Model)
    {
        <tr>
            <td>@game.Name</td>
        </tr>
    }

</table>

@if (Model.Count == 0)
{
    <p>No games to display</p>
}